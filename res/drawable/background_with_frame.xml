<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item >
        <bitmap android:src="@drawable/background_blue_green" />
    </item> 
    <item> 
        <shape android:shape="rectangle" >
    
		    <stroke
		        android:width="5dp"
		        android:color="#25383c" />
		    <solid android:color="@android:color/transparent" /> <!-- Solid transparent color is needed because some devices will turn the area in between the stroke black -->
	    </shape>
    </item>
</layer-list>
